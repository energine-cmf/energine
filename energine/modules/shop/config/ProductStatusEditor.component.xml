<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<methods>
		<method name="main">
			<javascript>
				<object name="GridManager" />
			</javascript>
			<fields>
				<field name="ps_id"/>
				<field name="ps_name"/>
                <field name="ps_is_default"/>
			</fields>			
			<toolbar>
				<control id="add" title="BTN_ADD" type="button" onclick="add"/>
				<control id="edit" title="BTN_EDIT" type="button" onclick="edit"/>
				<control id="delete" title="BTN_DELETE" type="button" onclick="del"/>
			</toolbar>
		</method>
		<method name="view">
		     <uri_patterns>
		         <pattern>/[int]/</pattern>
		     </uri_patterns>
			<javascript>
				<object name="Form"/>
			</javascript>
			<toolbar>
				<control id="list" title="BTN_CLOSE" type="button" onclick="close"/>
			</toolbar>
		</method>
		<method name="add">
		     <uri_patterns>
		         <pattern>/add/</pattern>
		     </uri_patterns>
			<javascript>
				<object name="Form"/>
			</javascript>
			<toolbar>
				<control id="save" title="BTN_SAVE" type="button" onclick="save"/>
				<control id="list" title="BTN_CLOSE" type="button" onclick="close"/>
			</toolbar>
		</method>
		<method name="edit">
		     <uri_patterns>
		         <pattern>/[int]/edit/</pattern>
		     </uri_patterns>
			<javascript>
				<object name="Form"/>
			</javascript>
			<toolbar>
				<control id="save" title="BTN_SAVE" type="button" onclick="save"/>
				<control id="list" title="BTN_CLOSE" type="button" onclick="close"/>
			</toolbar>
		</method>
		<method name="save" rights="2">
		     <uri_patterns>
		         <pattern>/save/</pattern>
		     </uri_patterns>
		</method>
		<method name="upload"  rights="2">
		     <uri_patterns>
		         <pattern>/upload/</pattern>
		     </uri_patterns>
		</method>
		<method name="delete"  rights="2">
		     <uri_patterns>
		         <pattern>/[int]/delete/</pattern>
		     </uri_patterns>
		</method>
		<method name="getRawData"  rights="2">
			<uri_patterns>
		         <pattern>/get-data/</pattern>
					<pattern>/get-data/page-[int]/</pattern>
		     </uri_patterns>
		</method>
		<method name="imageManager" rights="2">
			<uri_patterns>
				<pattern>/imagemanager/</pattern>
			</uri_patterns>
		</method>
		<method name="fileLibrary" rights="2">
			<uri_patterns>
				<pattern>/file-library/</pattern>
				<pattern>/file-library/[string]/</pattern>
			</uri_patterns>
		</method>
		<method name="cleanup" rights="2">
			<uri_patterns>
				<pattern>/cleanup/</pattern>
			</uri_patterns>
		</method>
<!-- 		<method name="up" rights="2">
			<uri_patterns>
				<pattern>/[int]/up/</pattern>
			</uri_patterns>
		</method>
		<method name="down" rights="2">
			<uri_patterns>
				<pattern>/[int]/down/</pattern>
			</uri_patterns>
		</method> -->
	</methods>
</configuration>