{
    "name": "energine/cmf",
    "description": "Energine is an XML/XSLT content management framework.",
    "type":"project",
    "license": "MIT",
    "keywords": ["xml", "xslt", "framework", "cmf"],
    "homepage": "http://energine.net/",
    "require": {
        "php": ">=5.4.0",
        "ext-dom":"*",
        "ext-gd":"*",
        "ext-json":"*",
        "ext-libxml":"*",
        "ext-mbstring":"*",
        "ext-PDO":"*",
        "ext-pdo_mysql":"*",
        "ext-SimpleXML":"*",
        "ext-session":"*",
        "ext-tidy":"*",
        "ext-xsl":"*"
    },
    "autoload": {
        "psr-4": {
            "Energine\\": "starter/core/modules",
            "EnergineSite\\": "starter/site/modules"
        }
    },
